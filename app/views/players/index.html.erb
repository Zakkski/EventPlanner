<div class="admin-player-list">
  <% @players.each do |player| %>
    <div class="player-card">
      <h2><%= player.name %></h2>
      <%= link_to "Delete", event_player_path(player.event, player), method: :delete, data: { confirm: "Are You sure?" }, class: "logout-button" %>
    </div>
  <% end %>
  <%= link_to "Back", event_path(@players.first.event), class: "main-button" %>
</div>
